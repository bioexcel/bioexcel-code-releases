      <memberdef kind="function" id="gpp__nextnb_8cpp_1a54589494f1c756cd53c1f4ca96e993c6" prot="public" static="yes" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>bool</type>
        <definition>static bool atom_is_present_in_nnb</definition>
        <argsstring>(const t_nextnb *nnb, int atom, int highest_order, int query)</argsstring>
        <name>atom_is_present_in_nnb</name>
        <param>
          <type>const <ref refid="structt__nextnb" kindref="compound">t_nextnb</ref> *</type>
          <declname>nnb</declname>
        </param>
        <param>
