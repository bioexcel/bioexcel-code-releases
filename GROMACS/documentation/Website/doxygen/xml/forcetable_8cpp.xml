        <definition>void table_spline3_fill_ewald_lr</definition>
        <argsstring>(real *table_f, real *table_v, real *table_fdv0, int ntab, double dx, real beta, real_space_grid_contribution_computer v_lr)</argsstring>
        <name>table_spline3_fill_ewald_lr</name>
        <param>
          <type><ref refid="real_8h_1a58a0c7cf2501f4492da833421be92547" kindref="member">real</ref> *</type>
