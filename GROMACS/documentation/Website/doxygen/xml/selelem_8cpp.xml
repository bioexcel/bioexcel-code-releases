        <name>_gmx_selelem_set_vtype</name>
        <param>
          <type>const <ref refid="namespacegmx_1a2804c9c82f2a9bd0ec94d3c2f1003f42" kindref="member">gmx::SelectionTreeElementPointer</ref> &amp;</type>
