      </memberdef>
      <memberdef kind="function" id="classgmx_1_1TrajectoryAnalysisSettings_1a0dc0e7e0104c46ee59f3391d7969a849" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>void</type>
        <definition>void gmx::TrajectoryAnalysisSettings::setFrameFlags</definition>
        <argsstring>(int frflags)</argsstring>
        <name>setFrameFlags</name>
        <param>
          <type>int</type>
